             xmlns:local="clr-namespace:Urus.DemoWindow"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Urus.UI;component/Themes/Generic.xaml"/>

                <ResourceDictionary Source="Styles/GlobalStyle.xaml"/>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
